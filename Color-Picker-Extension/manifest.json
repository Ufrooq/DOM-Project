{
  "name": "Color Picker",
  "description": "This a color picker extension that will extract #id of any thing on the screen.",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "./files/icon16.png",
    "32": "./files/icon32.png",
    "48": "./files/icon48.png",
    "128": "./files/icon128.png"
  }
}
